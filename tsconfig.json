{
  "compilerOptions": {
    "allowSyntheticDefaultImports": true,
    "noImplicitAny": true,
    "module": "es6",
    "target": "es5",
    "allowJs": true,
    "moduleResolution": "NodeNext",
    "baseUrl": ".", // This must be specified if "paths" is.
    "paths": {
      "WebAA": ["lib/WebAA"] // This mapping is relative to "baseUrl"
    }
  },
  "include": ["src/**/*.ts", "lib/**/*.ts"],
}
